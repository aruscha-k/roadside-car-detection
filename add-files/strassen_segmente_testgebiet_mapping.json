{
    "transmission_order" : [
        "streets",
        "segments"
    ],
    "tables_primary_keys" : [
        {"table" : "streets", "key" : "id"},
        {"table" : "segments", "key" : "id"}
    ],
    "path_to_data_set": ["features"],
    "mapping" : [
        {
            "path_to_property": ["properties", "objectid"],
            "postgres": {"table": "segments", "column": "id"}
        },
        {
            "path_to_property": ["properties", "str_nr"],
            "postgres": {"table": "segments", "column": "street_id"}
        },
        {
            "path_to_property": ["properties", "sstrgsname"],
            "postgres": {"table": "segments", "column": "sstrgsname"}
        },
        {
            "path_to_property": ["properties", "segm_gid"],
            "postgres": {"table": "segments", "column": "segm_gid"}
        },
        {
            "path_to_property": ["properties", "ot_name"],
            "postgres": {"table": "segments", "column": "ot_name"}
        },
        {
            "path_to_property": ["geometry", "type"],
            "postgres": {"table": "segments", "column": "geom_type"}
        },
        {
            "path_to_property": ["geometry", "coordinates"],
            "postgres": {"table": "segments", "column": "geom_coordinates"},
            "transformation" : "coordinates_to_json"
        },
        {
            "path_to_property": ["geometry"],
            "postgres": {"table": "segments", "column": "geometry"},
            "transformation" : "geom_to_json"    
        },
        {
            "path_to_property": ["properties", "laenge_m"],
            "postgres": {"table": "segments", "column": "laenge_m"}
        },
        {
            "path_to_property": ["properties", "str_nr"],
            "postgres": {"table": "streets", "column": "id"}
        },
        {
            "path_to_property": ["properties", "str"],
            "postgres": {"table": "streets", "column": "name"}
        }
    ]
}